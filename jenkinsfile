pipeline {
    agent any 
    tools {
        jdk 'JDK_17'
    }
    stages {
        stage ('vcs') {
          agent { label 'GIT' } 
          steps {
            sh 'git --version',
            git url: 'https://github.com/ajayreddy123456/spring-petclinic.git',
            branch: 'pet'
          }
          }
        }
        }
