pipeline {
     agent { label 'JDK_17' }
     stages {
        stage ('vcs') {
            steps {
                git url: 'https://github.com/ajayreddy123456/spring-petclinic.git',
                branch: 'spring',
                poll: true
            }
        }
        stage ('package') {
            steps {
                sh 'mvn package'
            }
        }
     }
}